{
  "name": "@pearson-components/features-list",
  "version": "2.1.9",
  "description": "Takes structured JSON payload and renders titled list of paragraphs with optional labeled HTML launch button and related image.",
  "main": "./main.js",
  "keywords": [
    "pearson-components",
    "features-list"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Pearson-Higher-Ed/features-list.git"
  },
  "scripts": {
    "test": "echo No unit testing yet available!",
    "dev": "webpack-dev-server --devtool eval --hot --progress --colors",
    "build": "webpack -p",
    "dev-build": "webpack",
    "version": "npm run gen-changelog && git add CHANGELOG.md",
    "ci": "npm run build",
    "gen-changelog": "node ./npm_scripts/gen-changelog.js",
    "release": "node ./npm_scripts/release.js",
    "postpublish": "npm access public && echo 'Package scope set to public!'"
  },
  "devDependencies": {
    "babelify": "^5.0.4",
    "conventional-changelog": "^1.1.0",
    "css-loader": "^0.23.1",
    "debowerify": "^1.4.1",
    "expect.js": "^0.3.1",
    "karma": "^0.12.31",
    "karma-browserify": "^4.0.0",
    "karma-mocha": "^0.1.10",
    "karma-phantomjs-launcher": "^0.1.4",
    "mocha": "^2.1.0",
    "node-sass": "^3.4.2",
    "sass-loader": "^3.2.0",
    "semver": "^5.1.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  }
